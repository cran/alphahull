@article{alphahull,
 title={Generalizing the Convex Hull of a Sample: The R Package alphahull},
 volume={34},
 url={https://www.jstatsoft.org/index.php/jss/article/view/v034i05},
 doi={10.18637/jss.v034.i05},
 abstract={This paper presents the R package &amp;lt;strong&amp;gt;alphahull&amp;lt;/strong&amp;gt; which implements the a-convex hull and the a-shape of a finite set of points in the plane. These geometric structures provide an informative overview of the shape and properties of the point set. Unlike the convex hull, the a-convex hull and the a-shape are able to reconstruct non-convex sets. This flexibility make them specially useful in set estimation. Since the implementation is based on the intimate relation of theses constructs with Delaunay triangulations, the R package &amp;lt;strong&amp;gt;alphahull&amp;lt;/strong&amp;gt; also includes functions to compute Voronoi and Delaunay tesselations. The usefulness of the package is illustrated with two small simulation studies on boundary length estimation.},
 number={5},
 journal={Journal of Statistical Software},
 author={Pateiro-L\'{o}pez, Beatriz and Rodr\'{i}guez-Casal, Alberto},
 year={2010},
 pages={1-28}
}
